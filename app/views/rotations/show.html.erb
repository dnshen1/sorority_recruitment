<div class="page-header">
  <h3>Rotation #<%= @rotation.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Bump group</dt>
      <dd><%= @rotation.bump_group_id %></dd>

      <dt>Party</dt>
      <dd><%= @rotation.party_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/rotations" class="btn btn-primary">
        Back
      </a>
      <a href="/rotations/<%= @rotation.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_rotation/<%= @rotation.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
